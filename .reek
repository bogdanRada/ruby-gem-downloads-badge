---
exclude_paths:
- middleware

TooManyInstanceVariables:
  enabled: false
TooManyMethods:
  enabled: false

ManualDispatch:
  enabled: false # disabled for force_encoding
InstanceVariableAssumption:
  enabled: false # disabled for CoreApi @base_url usage
DataClump:
  enabled: false # disable for CoreApi taking parameters ['options', 'url'] to 3 methods
